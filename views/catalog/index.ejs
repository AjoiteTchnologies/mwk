Catalog Index Page
<table>
	<thead>
		<tr>
			<th>id</th>
			<th>name</th>
			<th>price</th>
			<th>productid</th>
			<th>Action</th>
		</tr>
	</thead>
	<tbody>
		<% list.forEach(function(list){ %>
		<tr>
			<td><%= list.id %></td>
			<td><%= list.productname %></td>
			<td><%= list.price %></td>
			<td><%= list.productid %></td>
			<td><a href="/product/<%=list.id %>">View</a></td>
		</tr>
		<% }) %>
	</tbody>
</table>