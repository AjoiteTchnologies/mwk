<h1> USER LIST</h1>
<table>
	<thead>
		<tr>
			<th>User</th>
		</tr>
	</thead>
	<tbody>

		<% user.forEach(function(user){ %>
			<tr>
				<td><%- user.username %></td>
				<td><%- user.password %></td>
				<td><%- user.email %></td>
				<td><%- user.mobile %></td>
				<td></td>
			</tr>
		<%	}) %>
	</tbody>	

</table>
